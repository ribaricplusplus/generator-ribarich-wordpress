{
  "name": "generator-ribarich-wordpress",
  "version": "1.0.2",
  "description": "Generate a WordPress plugin or theme",
  "homepage": "",
  "author": {
    "name": "Bruno Ribaric",
    "email": "bruno@ribarich.me",
    "url": "https://ribarich.me/"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.mjs",
  "keywords": [
    "",
    "yeoman-generator"
  ],
  "devDependencies": {
    "coveralls": "^3.1.1",
    "eslint": "^9.6.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-xo": "^0.45.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "fast-glob": "^3.3.2",
    "generator-node": "^2.8.0",
    "get-latest-version": "^5.1.0",
    "lodash": "^4.17.21",
    "yeoman-generator": "^7.3.2",
    "yosay": "^3.0.0"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest"
  },
  "repository": "https://github.com/ribaricplusplus/generator-ribarich-wordpress",
  "license": "MIT"
}
